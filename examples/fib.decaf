class fibonacci {
    int fib(int var1) {
        int var2;
        if (var1==0)
        {
            var2=1;
        }
        else
        {
            if (var1==1)
            {
                var2=1;
            }
            else
            {
                var2 = fib(var1-1)+fib(var2-2);
            }
        }
        return var2;
    }

    void main () {
        int a;

        fib(a);
    }
}